<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.jay.learnspringboot.mapper.CityMapper">
    <resultMap id="CityMap" type="City">
        <id column="city_id" jdbcType="SMALLINT" property="cityId" javaType="Integer" />
        <id column="city" jdbcType="VARCHAR" property="city" javaType="String" />
        <id column="country_id" jdbcType="SMALLINT" property="countryId" javaType="Integer" />
        <id column="last_update" jdbcType="TIMESTAMP" property="lastUpdate" javaType="DATE" />
    </resultMap>
</mapper>